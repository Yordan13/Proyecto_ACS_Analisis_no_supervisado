package Futbol;

import java.io.File;

import org.opencv.core.Mat;
import org.opencv.imgcodecs.Imgcodecs;

public class Main {
  public static void main(String[] args) {
    AbstractFileManager fileManager = new FutbolFileManager();
    AbstractVideo video = new FootballVideo(fileManager.open("video.mp4"));
    AbstractFrame frame = video.getFrame();
    
    byte[] bytes = FileUtils.readFileToByteArray(new File("aaa.jpg"));
    Mat mat = Imgcodecs.imdecode(new MatOfByte(bytes), Imgcodecs.CV_LOAD_IMAGE_UNCHANGED);
  }
}
